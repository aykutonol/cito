            <joint name="base_slide_joint_x" pos="0 0 0" axis="1 0 0" type="slide" damping="0.7" />
            <joint name="base_slide_joint_y" pos="0 0 0" axis="0 1 0" type="slide" damping="0.7" />
            <joint name="base_roll_joint" pos="0 0 0" axis="0 0 1" damping="0.7" />
            <body name="base_r_drive_wheel_link" pos="-0.11 -0.133 0.04">
