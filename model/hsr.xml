            </body>
        </body>
    </worldbody>

    <!-- Actuators -->
    <actuator>
        <velocity name="base_x" joint="base_slide_joint_x" ctrllimited="true" ctrlrange="-2 2" />
        <velocity name="base_y" joint="base_slide_joint_y" ctrllimited="true" ctrlrange="-2 2" />
        <velocity name="base_roll" joint="base_roll_joint" ctrllimited="true" ctrlrange="-2 2" />
    </actuator>

    <!-- Contacts -->
    <contact>
        <pair geom1='floor' geom2='base' />
        <pair geom1='base' geom2='object' />
        <pair geom1='floor' geom2='object' />
    </contact>

    <!-- Key frames -->
    <keyframe>
        <key name="init_pose" qpos="0.5 0 0.05 1 0 0 0 0 0 0" />
    </keyframe>

</mujoco>
